program Project1;

uses
  System.StartUpCopy,
  FMX.Forms,
  Login in 'Login.pas' {Form1},
  UDB in 'DB\UDB.pas' {DataModule2: TDataModule},
  Cadastro in 'Cadastro.pas' {Form2};

